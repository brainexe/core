{
  "name": "brainexe/core",
  "description": "Framework core - used for brainexe/homie",
  "homepage": "https://mdoetsch.de",
  "minimum-stability": "beta",
  "license": "MIT",
  "authors": [
    {
      "name": "Matze",
      "email": "matze@mdoetsch.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "BrainExe\\Core\\": "src/",
      "BrainExe\\Tests\\": "Tests/BrainExe/"
    }
  },
  "type": "library",
  "require": {
    "php": ">=7.1.0",
    "symfony/cache": "~3.3",
    "symfony/config": "~3.3",
    "symfony/console": "~3.3",
    "symfony/dependency-injection": "~3.3",
    "symfony/expression-language": "~3.3",
    "symfony/finder": "~3.3",
    "symfony/http-foundation": "~3.3",
    "symfony/http-kernel": "~3.3",
    "symfony/process": "~3.3",
    "symfony/routing": "~3.3",
    "symfony/proxy-manager-bridge": "~3.3",
    "ocramius/proxy-manager": "~2.1",
    "doctrine/annotations": "~1.4|~2.0",
    "monolog/monolog": "~1.22",
    "predis/predis": "~1.1",
    "mtdowling/cron-expression": "~1.1",
    "phpmailer/phpmailer": "~5.2",
    "guzzlehttp/guzzle": "~6.2",
    "christian-riesen/base32": "~1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "suggest": {
    "johnkary/phpunit-speedtrap": "~1.0@dev",
    "lstrojny/phpunit-clever-and-smart": "~0.5"
  },
  "bin": [
    "console"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  }
}
