{
  "name": "brainexe/core",
  "description": "Framework core",
  "homepage": "https://mdoetsch.de",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Matze",
      "email": "matze@mdoetsch.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "BrainExe\\Core\\": "src/",
      "BrainExe\\Tests\\": "Tests/BrainExe/"
    }
  },
  "type": "library",
  "require": {
    "symfony/config": "2.6.*",
    "symfony/console": "2.6.*",
    "symfony/dependency-injection": "2.6.*",
    "symfony/finder": "2.6.*",
    "symfony/http-foundation": "2.6.*",
    "symfony/http-kernel": "2.6.*",
    "symfony/process": "2.6.*",
    "symfony/routing": "~2.6",
    "symfony/security": "~2.6",
    "symfony/expression-language": "~2.6",
    "brainexe/annotations": "dev-master",
    "brainexe/message-queue": "dev-master",
    "brainexe/template": "dev-master",
    "monolog/monolog": "1.12.*",
    "doctrine/cache": "1.4.*",
    "predis/predis": "1.1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*",
    "johnkary/phpunit-speedtrap": "~1.0@dev",
    "lstrojny/phpunit-clever-and-smart": "0.*"
  },
  "suggest": {
    "crunch/inotify": "needed for 'console debug'"
  },
  "bin": [
    "console"
  ]
}
