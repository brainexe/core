{
  "name": "brainexe/core",
  "description": "Framework core",
  "homepage": "https://mdoetsch.de",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Matze",
      "email": "matze@mdoetsch.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "BrainExe\\Core\\": "src/",
      "BrainExe\\Tests\\": "Tests/BrainExe/"
    }
  },
  "type": "library",
  "require": {
    "symfony/config": "~2.7",
    "symfony/console": "~2.7",
    "symfony/dependency-injection": "~2.7",
    "symfony/finder": "~2.7",
    "symfony/http-foundation": "~2.7",
    "symfony/http-kernel": "~2.7",
    "symfony/process": "~2.7",
    "symfony/routing": "~2.7",
    "brainexe/annotations": "*",
    "brainexe/message-queue": "*",
    "monolog/monolog": "~1.12",
    "doctrine/cache": "~1.4",
    "predis/predis": "~1.1",
    "mtdowling/cron-expression": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7",
    "johnkary/phpunit-speedtrap": "~1.0@dev",
    "lstrojny/phpunit-clever-and-smart": "0.*"
  },
  "bin": [
    "console"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
