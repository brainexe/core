{
  "name": "brainexe/core",
  "description": "Framework core - used for brainexe/homie",
  "homepage": "https://mdoetsch.de",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Matze",
      "email": "matze@mdoetsch.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "BrainExe\\Core\\": "src/",
      "BrainExe\\Tests\\": "Tests/BrainExe/"
    }
  },
  "type": "library",
  "require": {
    "symfony/config": "~2.7|~3.0",
    "symfony/console": "~2.7|~3.0",
    "symfony/dependency-injection": "~2.7|~3.0",
    "symfony/finder": "~2.7|~3.0",
    "symfony/http-foundation": "~2.7|~3.0",
    "symfony/http-kernel": "~2.7|~3.0",
    "symfony/process": "~2.7|~3.0",
    "symfony/routing": "~2.7|~3.0",
    "symfony/proxy-manager-bridge": "~2.7|~3.0",
    "zendframework/zend-stdlib": "~2.7|~3.0",
    "brainexe/annotations": "~1.1",
    "monolog/monolog": "~1.17",
    "doctrine/cache": "~1.5",
    "predis/predis": "~1.1",
    "mtdowling/cron-expression": "1.0.*",
    "phpmailer/phpmailer": "~5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.1"
  },
  "suggest": {
    "johnkary/phpunit-speedtrap": "~1.0@dev",
    "lstrojny/phpunit-clever-and-smart": "0.*"
  },
  "bin": [
    "console"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
