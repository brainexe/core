{
	"name": "brainexe/core",
	"description": "Framework core",
	"minimum-stability": "dev",
	"version": "1.0.22",
	"authors": [
		{
			"name": "Matze",
			"email": "matze@mdoetsch.de"
		}
	],
	"autoload": {
		"psr-4": {
			"BrainExe\\Core\\": "src/"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "http://satis.mdoetsch.de"
		}
	],
	"require": {
		"symfony/config": "~2.5",
		"symfony/console": "~2.5",
		"symfony/dependency-injection": "~2.5",
		"symfony/finder": "~2.5",
		"symfony/http-foundation": "~2.5",
		"symfony/http-kernel": "~2.5",
		"symfony/process": "~2.5",
		"symfony/routing": "~2.5",
		"symfony/security": "~2.5",

		"brainexe/annotations": "dev-master",
		"brainexe/message-queue": "dev-master",
		"brainexe/template": "dev-master",

		"monolog/monolog": "~1.9",
		"doctrine/cache": "~1.3",
		"christian-riesen/base32": "~1.1"
	},

	"require-dev": {
	  "phpunit/phpunit": "4.3.*"
	},

	"suggest": {
		"crunch/inotify": "needed for 'console debug'"
	},

	"bin": [
		"console"
	]
}
