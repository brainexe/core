{
	"name": "matze/core",
	"description": "Framework core",
	"minimum-stability": "dev",
	"version": "1.0.6",
	"authors": [
		{
			"name": "Matze",
			"email": "matze@mdoetsch.de"
		}
	],
	"autoload": {
		"psr-4": {
			"Matze\\Core\\": "src/"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "http://satis.mdoetsch.de"
		}
	],
	"require": {
		"symfony/config": "~2.4",
		"symfony/console": "~2.4",
		"symfony/dependency-injection": "~2.4",
		"symfony/finder": "~2.4",
		"symfony/http-foundation": "~2.4",
		"symfony/http-kernel": "~2.4",
		"symfony/process": "~2.4",
		"symfony/routing": "~2.4",
		"symfony/security": "~2.4",

		"brainexe/annotations": "dev-master",
		"matze/template": "dev-master",

		"monolog/monolog": "~1.9",
		"doctrine/cache": "~1.3"
	},

	"suggest": {
		"crunch/inotify": "needed for 'console debug'"
	},

	"bin": [
		"console"
